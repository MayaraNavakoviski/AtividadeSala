programa
{
	
	funcao inicio()
	{
		inteiro numeros[3][3] = {
		 	{1, 3, 5},
		 	{7, 9, 11},
		 	{13, 15, 17}
		 }

		 inteiro soma = 0
		
		para(inteiro y = 0; y < 3; y++){
      	  para(inteiro i = 0; i < 3; i++){
	  	escreva(numeros[y][i], " ")
	  	soma = soma + numeros[y][i]
	  	}
           escreva("\n")
   }
     escreva("\n")

       escreva("O resultado da soma da matriz foi: " , soma)
	}
}
